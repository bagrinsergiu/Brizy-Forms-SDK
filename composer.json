{
  "name": "bagrinsergiu/brizy-forms-sdk",
  "description": "Brizy form's PHP SDK",
  "keywords": [
    "brizy",
    "forms"
  ],
  "type": "library",
  "require": {
    "php": ">=7",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "drewm/mailchimp-api": "~2.2",
    "sendinblue/api-v3-sdk": "~6.0",
    "campaignmonitor/createsend-php": "dev-master",
    "guzzlehttp/guzzle": "^6.5",
    "activecampaign/api-php": "2.0.*",
    "psr/log": "^1.0.0",
    "drewm/drip": "0.8.*",
    "phpfui/constantcontact": "dev-php7"
  },
  "require-dev": {
    "ext-dom": "*",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-0": {
      "BrizyForms": "lib/"
    }
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/alecszaharia/ConstantContact.git"
    }
  ]
}

