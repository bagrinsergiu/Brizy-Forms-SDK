{
  "name": "bagrinsergiu/brizy-forms-sdk",
  "description": "Brizy form's PHP SDK",
  "keywords": ["brizy", "forms"],
  "type": "library",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "drewm/mailchimp-api": "~2.2",
    "sendinblue/api-v3-sdk": "~6.0",
    "campaignmonitor/createsend-php": "dev-master",
    "guzzlehttp/guzzle": "6.3.*",
    "activecampaign/api-php": "2.0.*",
    "psr/log": "^1.0.0",
    "drewm/drip": "0.8.*",
    "google/apiclient": "^2.11"
  },
  "require-dev": {
    "ext-dom": "*",
    "phpunit/phpunit": "^4.8"
  },
  "scripts": {
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "Sheets"
    ]
  },
  "autoload": {
    "psr-0": {
      "BrizyForms": "lib/"
    }
  }
}
